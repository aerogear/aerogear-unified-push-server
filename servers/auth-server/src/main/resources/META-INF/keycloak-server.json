{
    "admin": {
        "realm": "master"
    },

    "model": {
        "provider": "jpa"
    },

    "modelCache": {
        "provider": "${keycloak.model.cache.provider:none}"
    },

    "timer": {
        "provider": "basic"
    },

    "theme": {
        "default": "keycloak",
        "staticMaxAge": 2592000,
        "cacheTemplates": true,
        "folder": {
          "dir": "${jboss.server.config.dir}/themes"
        }
    },

    "login-forms": {
        "provider": "freemarker"
    },

    "account": {
        "provider": "freemarker"
    },

    "email": {
        "provider": "freemarker"
    },

    "scheduled": {
        "interval": 900
    }
}